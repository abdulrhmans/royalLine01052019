<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="res_config_settings_view_form" model="ir.ui.view">
		<field name="name">res.config.settings</field>
		<field name="model">res.config.settings</field>
		<field name="inherit_id"
			ref="hr_payroll.res_config_settings_view_form" />
		<field name="arch" type="xml">
			<div id="hr_payroll_accountant" position='inside'>
				<div class="col-md-6 col-xs-12 o_setting_box">
					<div class="o_setting_left_pane">
					</div>
					<div class="o_setting_right_pane">
						<label for="eosb_account_id" />
						<field name="eosb_account_id" options="{'no_create': True,'no_quick_create': True,'no_open': True, 'no_create_edit': True}" />
					</div>
				</div>
			</div>
		</field>
	</record>
</odoo>
